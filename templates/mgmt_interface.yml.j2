---
# Management Interface IP Addresses for {{ fabric_name }}
# Generated by: 01-profile-existing-switches.yml
# 
# Contains the management IP address assigned to each switch's management interface
# Management interface is defined by the 'mgmt_int' variable (e.g., Vlan199)

switches:
{% for switch in mgmt_interfaces %}
  {{ switch.hostname }}:
    mgmt_interface: "{{ switch.mgmt_interface }}"
    mgmt_ip_address: "{{ switch.mgmt_ip_address }}"
    mgmt_interface_vrf: "{{ switch.mgmt_interface_vrf }}"
    mgmt_static_default: "{{ switch.mgmt_static_default }}"
{% endfor %}
